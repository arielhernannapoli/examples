<Window x:Class="WPFTest.View.ReportingServicesWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPFTest.View"
        xmlns:rv="clr-namespace:Microsoft.Reporting.WinForms;assembly=Microsoft.ReportViewer.WinForms"
        xmlns:wf="clr-namespace:System.Windows.Forms;assembly=System.Windows.Forms"
        mc:Ignorable="d"      
        x:Name="Reporting_Services_Window"
        Title="ReportingServicesWindow" Height="500" Width="600">
    <Grid>
        <StackPanel Grid.Row="1">
            <WindowsFormsHost Height="200">
                <rv:ReportViewer x:Name="_reportViewer"/>
            </WindowsFormsHost>
        </StackPanel>
        <Button Grid.Row="2" Height="25" Width="150" Content="Volver" Command="{Binding ReturnCommand}" CommandParameter="{Binding ElementName=Reporting_Services_Window}" Margin="5" Focusable="False"/>
    </Grid>
</Window>
